{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2020"],
    "module": "commonjs",
    "outDir": "./dist",
    "preserveSymlinks": false,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2020",
    "useUnknownInCatchVariables": false
  },
  "exclude": ["node_modules"]
}
